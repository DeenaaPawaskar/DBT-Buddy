// JavaScript Example: Reading Entities
// Filterable fields: name, aadhaar_last_4, phone, email, school_id, class_grade, dbt_status, last_check_date, has_opted_for_reminders
async function fetchStudentEntities() {
    const response = await fetch(`https://app.base44.com/api/apps/68bafc0a417c0659ded4215b/entities/Student`, {
        headers: {
            'api_key': '967c489ab00647cf8e32e15e0294452d', // or use await User.me() to get the API key
            'Content-Type': 'application/json'
        }
    });
    const data = await response.json();
    console.log(data);
}

// JavaScript Example: Updating an Entity
// Filterable fields: name, aadhaar_last_4, phone, email, school_id, class_grade, dbt_status, last_check_date, has_opted_for_reminders
async function updateStudentEntity(entityId, updateData) {
    const response = await fetch(`https://app.base44.com/api/apps/68bafc0a417c0659ded4215b/entities/Student/${entityId}`, {
        method: 'PUT',
        headers: {
            'api_key': '967c489ab00647cf8e32e15e0294452d', // or use await User.me() to get the API key
            'Content-Type': 'application/json'
        },
        body: JSON.stringify(updateData)
    });
    const data = await response.json();
    console.log(data);
}